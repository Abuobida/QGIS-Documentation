#: ../../source/docs/user_manual/working_with_ogc/server/config.rst:36
msgid "1 WARNING,"
msgstr ""

#: ../../source/docs/user_manual/working_with_ogc/server/config.rst:37
msgid ""
"2 CRITICAL (log just critical errors, suitable for production purposes)."
msgstr ""

#: ../../source/docs/user_manual/working_with_ogc/server/config.rst:39
msgid "Example:"
msgstr ""

